{
  "name": "stimulus-x",
  "version": "0.0.1",
  "description": "Reactivity engine for Stimulus JS",
  "author": "Mark Perkins",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/allmarkedup/stimulus-x/issues"
  },
  "homepage": "https://github.com/allmarkedup/stimulus-x#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/allmarkedup/stimulus-x.git"
  },
  "source": "src/index.js",
  "main": "dist/main.js",
  "cdn": "dist/cdn.js",
  "demo": "dist/demo.html",
  "type": "module",
  "targets": {
    "demo": {
      "source": "demo/index.html"
    },
    "cdn": {
      "context": "browser",
      "includeNodeModules": true,
      "engines": {
        "browsers": "> 0.5%, last 2 versions, not dead"
      }
    },
    "main": {
      "includeNodeModules": true
    }
  },
  "keywords": [
    "stimulus",
    "stimulusjs",
    "reactive",
    "bindings",
    "dom"
  ],
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "serve": "parcel serve --target=demo",
    "test": "node --experimental-vm-modules --disable-warning=ExperimentalWarning node_modules/jest/bin/jest.js",
    "release": "release-it"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.0",
    "@vue/reactivity": "^3.5.17",
    "dot-prop": "^9.0.0"
  },
  "devDependencies": {
    "@parcel/resolver-glob": "^2.15.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "parcel": "^2.15.2",
    "release-it": "^19.0.3"
  }
}
